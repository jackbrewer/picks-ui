{
  "name": "picks-ui",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://picks.jackbrewer.co.uk",
  "scripts": {
    "build:styles": "stylus -m -c -u stylus-mixins -u responsive-grid -u poststylus --with \"['autoprefixer']\" ./src/asset/stylus/index*.styl -o ./src/asset/css/",
    "build": "node scripts/build.js && sw-precache --config=sw-precache-config.js",
    "coverage": "npm test -- --coverage",
    "coverage:component": "npm test -- --coverage --collectCoverageFrom=src/component/**/*.js",
    "fix:js": "npm run js:lint -- --fix",
    "lint:js": "eslint .",
    "pretest": "concurrently --names \"Lint JS\" --prefix name \"npm run lint:js\" --prefix-colors \"bgYellow.bold\"",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "watch:styles": "npm run build:styles -- -w",
    "watch": "concurrently --names \"webpack,stylus\" --prefix name \"npm run start\" \"npm run watch:styles\" --prefix-colors \"bgBlue.bold,bgMagenta.bold\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "tidy": "rimraf build coverage"
  },
  "devDependencies": {
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-preset-react-app": "^2.1.1",
    "babel-runtime": "^6.20.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.26.1",
    "detect-port": "1.0.1",
    "dotenv": "2.0.0",
    "enzyme": "^2.7.1",
    "eslint": "3.16.1",
    "eslint-config-react-app": "^0.6.1",
    "eslint-config-standard": "^7.0.0",
    "eslint-config-standard-jsx": "^3.3.0",
    "eslint-config-standard-react": "^4.3.0",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^2.1.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.0",
    "fs-extra": "0.30.0",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.3",
    "jest": "18.1.0",
    "json-loader": "0.5.4",
    "object-assign": "4.1.1",
    "postcss-loader": "1.2.2",
    "promise": "7.1.1",
    "react-addons-test-utils": "^15.4.2",
    "react-dev-utils": "^0.5.2",
    "style-loader": "0.13.1",
    "sw-precache": "^5.1.0",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.2"
  },
  "dependencies": {
    "autoprefixer": "^6.7.6",
    "classnames": "^2.2.5",
    "concurrently": "^3.4.0",
    "css-color-converter": "^1.1.0",
    "moment": "^2.17.1",
    "poststylus": "^0.2.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-element-to-jsx-string": "^6.3.0",
    "react-helmet": "^4.0.0",
    "react-router": "4.0.0-beta.6",
    "react-router-dom": "^4.0.0-beta.6",
    "react-syntax-highlighter": "^5.0.0",
    "responsive-grid": "^1.1.0",
    "rimraf": "^2.6.1",
    "sinon": "^1.17.7",
    "stylus": "^0.54.5",
    "stylus-mixins": "^0.4.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
